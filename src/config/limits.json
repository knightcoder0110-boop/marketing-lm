{
  "upload": {
    "maxSizeBytes": 10485760,
    "allowedFormats": ["image/jpeg", "image/png", "image/webp"],
    "maxDimensions": {
      "width": 2048,
      "height": 2048
    }
  },
  "generation": {
    "defaultResolutions": [
      { "name": "Square", "width": 1024, "height": 1024, "aspect": "1:1" },
      { "name": "Portrait", "width": 768, "height": 1024, "aspect": "3:4" },
      { "name": "Landscape", "width": 1024, "height": 768, "aspect": "4:3" },
      { "name": "Widescreen", "width": 1024, "height": 576, "aspect": "16:9" }
    ],
    "maxPromptLength": 500,
    "maxNegativePromptLength": 200
  },
  "jobs": {
    "maxConcurrent": 3,
    "timeoutMinutes": 10
  }
}
